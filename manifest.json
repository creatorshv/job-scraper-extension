{
  "manifest_version": 3,
  "name": "Listings",
  "version": "1.0",
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["*://*.linkedin.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/restart-16.png",
      "32": "icons/restart-32.png",
      "48": "icons/restart-48.png",
      "128": "icons/restart-128.png"
    }
  }
}
